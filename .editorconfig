root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_size = 4

[*.{csproj,vbproj,fsproj,cs,vb,fs,cmd,bat,ps1}]
end_of_line = crlf

[*.go]
indent_style = tab
